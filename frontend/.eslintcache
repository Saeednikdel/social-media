nk className={classes.navLink} to=\"/setting/profile\">\r\n                <Typography color=\"secondary\" variant=\"body1\">\r\n                  {translate(\"setting\")}\r\n                </Typography>\r\n              </Link>\r\n            )}\r\n          </div>\r\n        </div>\r\n        {profile.bio && (\r\n          <Typography style={{ marginTop: 10 }}>{profile.bio}</Typography>\r\n        )}\r\n        <div style={{ display: \"flex\", alignItems: \"center\" }}>\r\n          <Link\r\n            className={classes.navLinkFollower}\r\n            to={`/list/follower/${userName}`}\r\n          >\r\n            <Typography color=\"textSecondary\" variant=\"subtitle2\">\r\n              {translate(\"follower\")} : {profile.followers}\r\n            </Typography>\r\n          </Link>\r\n          <Link\r\n            className={classes.navLinkFollower}\r\n            to={`/list/following/${userName}`}\r\n          >\r\n            <Typography color=\"textSecondary\" variant=\"subtitle2\">\r\n              {translate(\"following\")} : {profile.followings}\r\n            </Typography>\r\n          </Link>\r\n        </div>\r\n      </div>\r\n      <Divider />\r\n      <div>\r\n        {profile.is_entity && (\r\n          <Tabs value={tab} indicatorColor=\"primary\" textColor=\"primary\">\r\n            {tabList.map((t) => (\r\n              <Tab\r\n                style={{ flexGrow: 1 }}\r\n                label={t.label}\r\n                value={t.value}\r\n                onClick={() => setTab(t.value)}\r\n              />\r\n            ))}\r\n          </Tabs>\r\n        )}\r\n        <Component value={tab} />\r\n      </div>\r\n    </>\r\n  ) : (\r\n    <>\r\n      <LinearProgress color=\"secondary\" />\r\n    </>\r\n  );\r\n};\r\n\r\nconst mapStateToProps = (state) => ({\r\n  isAuthenticated: state.auth.isAuthenticated,\r\n  user: state.auth.user,\r\n  profile: state.blog.profile,\r\n});\r\nexport default withRouter(\r\n  connect(mapStateToProps, {\r\n   