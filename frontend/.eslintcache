sses = useStyles();\n  const [search, setSearch] = useState(getQueryVariable(\"keyword\"));\n  useEffect(() => {\n    load_jobs(1, getQueryVariable(\"keyword\"));\n    setPage(2);\n  }, []);\n\n  const submit = (e) => {\n    e.preventDefault();\n    const currentUrlParams = new URLSearchParams();\n    currentUrlParams.set(\"keyword\", search);\n    if (window.location.pathname === \"/jobs\") {\n      history.push(\n        window.location.pathname + \"?\" + currentUrlParams.toString()\n      );\n    } else {\n      window.location.replace(\"/?keyword=\" + search);\n    }\n    load_jobs(1, search);\n    setPage(2);\n  };\n  const fetchData = async () => {\n    await load_jobs(page, search);\n    setPage(page + 1);\n  };\n  return (\n    <div>\n      <Toolbar />\n      {jobs && (\n        <InfiniteScroll\n          dataLength={jobs.length}\n          next={fetchData}\n          hasMore={count > jobs.length}\n          loader={\n            <div className={classes.loader}>\n              <CircularProgress color=\"secondary\" />\n            </div>\n          }\n          endMessage={\n            <div className={classes.loader}>\n              <p>...</p>\n            </div>\n          }\n        >\n          {jobs.map((job) => (\n            <PostCard post={job} job={true} />\n          ))}\n        </InfiniteScroll>\n      )}\n      <div className={classes.form}>\n        <Toolbar />\n        <form autoComplete=\"off\" onSubmit={(e) => submit(e)}>\n          <TextField\n            autoComplete=\"off\"\n            id=\"search\"\n            placeholder={translate(\"search\")}\n            color=\"secondary\"\n            variant=\"outlined\"\n            className={classes.textField}\n            value={search ? search : \"\"}\n            onChange={(e) => setSearch(e.target.value)}\n            size=\"small\"\n            InputProps={{\n              endAdornment: (\n                <IconButton color=\"inherit\" size=\"small\" type=\"submit\">\n                  <Se